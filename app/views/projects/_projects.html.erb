<% projects.each do |project| %>
    <div class="col-md-4 mb-3">
      <div class="card project-card">
        <%= link_to project_path(project), class: 'stretched-link', data: { turbolinks: false } do %>
          <div class="card-body">
            <h5 class="card-title"><%= project.text %></h5>
          </div>
        <% end %>
      </div>
    </div>
<% end %>